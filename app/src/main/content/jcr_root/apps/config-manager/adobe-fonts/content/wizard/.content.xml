<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="Typekit Configurations"
        sling:resourceType="granite/ui/components/coral/foundation/page">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[cq.conf.admin.typekit.edit]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/page/body">
            <items jcr:primaryType="nt:unstructured">
                <form
                    granite:id="edit-configuration-properties-form"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form"
                    action="${requestPathInfo.suffix}"
                    dataPath="${requestPathInfo.suffix}/jcr:content"
                    foundationForm="{Boolean}true"
                    maximized="{Boolean}true"
                    method="post"
                    style="vertical">
                    <granite:data
                        jcr:primaryType="nt:unstructured"
                        config-type="adobe-fonts"
                        config-admin="/apps/config-manager/adobe-fonts/content/configurations.html/conf"
                        content-path="${requestPathInfo.suffix}"
                        post-url-edit="${requestPathInfo.suffix}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <wizard
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Adobe Fonts"
                            sling:resourceType="granite/ui/components/coral/foundation/wizard"
                            cancelHref="${empty header.Referer ? granite:encodeURIPath(&quot;/apps/adobe-fonts/content/configurations.html/conf&quot;) : header.Referer}">
                            <items jcr:primaryType="nt:unstructured">
                                <editingStep
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Edit"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <propertiesContainer
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                            <items jcr:primaryType="nt:unstructured">
                                                <propColumn
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <header
                                                            granite:class="settingsHeader"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                            level="2"
                                                            text="Project Settings"/>
                                                        <hrspan
                                                            granite:class="form-section-divider"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"/>
                                                        <!-- Hidden Fields -->
                                                        <primaryTypeContent
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                            name="./sling:resourceType"
                                                            value="config-manager/adobe-fonts/components/conf/page"/>
                                                        <tagComponentHeader
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                            name="./tagComponentHeader"
                                                            value="config-manager/adobe-fonts/components/tag/header"/>
                                                        <tagComponentFooter
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                            name="./tagComponentFooter"
                                                            value="config-manager/adobe-fonts/components/tag/footer"/>
                                                        <!-- Edit Fields -->
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Title"
                                                            name="./jcr:title"
                                                            required="{Boolean}true"/>
                                                        <projectId
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Project ID"
                                                            name="./projectId"
                                                            required="{Boolean}true"/>
                                                        <embedType
                                                            granite:class="embedType"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Embed Type"
                                                            name="./embedType">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <linkTag
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Link Tag"
                                                                    value="linkTag"/>
                                                                <styleTag
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Style Tag"
                                                                    value="styleTag"/>
                                                                <scriptTag
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Script Tag"
                                                                    value="scriptTag"/>
                                                            </items>
                                                        </embedType>
                                                    </items>
                                                </propColumn>
                                            </items>
                                        </propertiesContainer>
                                    </items>
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control dmconfig-submit-btn"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            text="Save"
                                            type="submit"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                </editingStep>
                            </items>
                        </wizard>
                    </items>
                </form>
            </items>
        </body>
    </jcr:content>
</jcr:root>
